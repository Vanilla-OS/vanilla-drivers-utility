msgstr "Ordre d'instal·lació: %s"

#: vanilla_drivers_utility/widgets/driver.py:45
msgid "Free"
msgstr "Gratuït"

#: vanilla_drivers_utility/widgets/driver.py:45
msgid "Proprietary"
msgstr "Propietària"

#: vanilla_drivers_utility/widgets/driver.py:46
